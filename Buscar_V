Sub BuscarData()

    Dim cont As Long
    Dim ultLinea As Long
    Dim cel As Variant
    Dim agentdyn As Variant
    Dim usuario As Variant
    Dim rango As Variant
    
    Application.ScreenUpdating = False
    
    ultLinea = Sheets("Ip's_Glovo").Range("A" & Rows.Count).End(xlUp).Row
    Set rango = Sheets("Glovo-Dynamicall").Range("B1:AC5000")
    Set rango1 = Sheets("Progra").Range("A1:N5000")
    
    For cont = 2 To ultLinea
    
        'BUSCARV NOMBRE
    
        usuario = Sheets("Ip's_Glovo").Cells(cont, 1)
        agentdyn = Application.VLookup(usuario, rango, 2, False)
        
        If IsError(agentdyn) Then
            agentdyn = "#N/A"
        End If
        
            Sheets("Ip's_Glovo").Cells(cont, 6) = agentdyn
            
        'BUSCARV CANAL
    
        usuario = Sheets("Ip's_Glovo").Cells(cont, 1)
        agentdyn = Application.VLookup(usuario, rango, 10, False)
        
        If IsError(agentdyn) Then
            agentdyn = "#N/A"
        End If
        
            Sheets("Ip's_Glovo").Cells(cont, 7) = agentdyn
            
        'BUSCARV SUPERVISOR
    
        usuario = Sheets("Ip's_Glovo").Cells(cont, 1)
        agentdyn = Application.VLookup(usuario, rango, 3, False)
        
        If IsError(agentdyn) Then
            agentdyn = "#N/A"
        End If
        
            Sheets("Ip's_Glovo").Cells(cont, 8) = agentdyn
            
         'BUSCARV DESCANSO
    
        usuario = Sheets("Ip's_Glovo").Cells(cont, 1)
        agentdyn = Application.VLookup(usuario, rango1, 6, False)
        
        If IsError(agentdyn) Then
            agentdyn = "#N/A"
        End If
        
            Sheets("Ip's_Glovo").Cells(cont, 9) = agentdyn
            
        'BUSCARV HORARIO
    
        usuario = Sheets("Ip's_Glovo").Cells(cont, 1)
        agentdyn = Application.VLookup(usuario, rango1, 7, False)
        
        If IsError(agentdyn) Then
            agentdyn = "#N/A"
        End If
        
            Sheets("Ip's_Glovo").Cells(cont, 10) = agentdyn
            
     Next cont
     
End Sub
