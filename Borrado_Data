Sub Borrado()

Application.ScreenUpdating = False
Application.DisplayAlerts = False

    Sheets("Ip's_Glovo").Select
    Rows("1:1").Select
    Selection.AutoFilter
    Cells.Select
    Selection.Clear
    Range("A1").Select
    Sheets("Glovo-Dynamicall").Select
    ActiveWindow.SelectedSheets.Delete
    Sheets("Progra").Select
    ActiveWindow.SelectedSheets.Delete
    Sheets("Ip's_Glovo").Select
    
End Sub
